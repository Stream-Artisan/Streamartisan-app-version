<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <!-- SEO Meta Tags -->
    <title><%= typeof title !== 'undefined' ? title : 'SoftEdigi - Professional Software Development & Digital Solutions' %></title>
    <meta name="description" content="<%= typeof description !== 'undefined' ? description : 'SoftEdigi offers professional software development, responsive design, and digital solutions. Transform your vision into stunning digital realities with our expert team.' %>">
    <meta name="keywords" content="<%= typeof keywords !== 'undefined' ? keywords : 'software development, web design, responsive design, digital solutions, website creation, professional websites, SoftEdigi' %>">
    <meta name="author" content="SoftEdigi">
    <meta name="robots" content="index, follow">
    
    <!-- Canonical URL -->
    <link rel="canonical" href="<%= typeof canonicalUrl !== 'undefined' ? canonicalUrl : 'https://softedigi.com' + (typeof currentPath !== 'undefined' ? currentPath : '') %>">
    
    <!-- Open Graph Meta Tags -->
    <meta property="og:title" content="<%= typeof title !== 'undefined' ? title : 'SoftEdigi - Professional Software Development Services' %>">
    <meta property="og:description" content="<%= typeof description !== 'undefined' ? description : 'Transform your vision into stunning digital realities with cutting-edge design and development.' %>">
    <meta property="og:type" content="website">
    <meta property="og:url" content="<%= typeof canonicalUrl !== 'undefined' ? canonicalUrl : 'https://softedigi.com' + (typeof currentPath !== 'undefined' ? currentPath : '') %>">
    <meta property="og:image" content="https://softedigi.com/images/og-image.jpg">
    <meta property="og:site_name" content="SoftEdigi">
    <meta property="og:locale" content="en_US">
    
    <!-- Twitter Card Meta Tags -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="<%= typeof title !== 'undefined' ? title : 'SoftEdigi - Professional Software Development Services' %>">
    <meta name="twitter:description" content="<%= typeof description !== 'undefined' ? description : 'Transform your vision into stunning digital realities with cutting-edge design and development.' %>">
    <meta name="twitter:image" content="https://softedigi.com/images/og-image.jpg">
    
    <!-- Enhanced Schema.org JSON-LD -->
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "Organization",
        "name": "SoftEdigi",
        "url": "https://softedigi.com",
        "logo": "https://softedigi.com/images/logo.png",
        "description": "Professional software development and digital solutions specializing in responsive websites, mobile apps, and innovative digital marketing strategies.",
        "foundingDate": "2020",
        "address": {
            "@type": "PostalAddress",
            "addressCountry": "US"
        },
        "contactPoint": {
            "@type": "ContactPoint",
            "telephone": "+1-555-0123",
            "contactType": "customer service",
            "availableLanguage": ["English"],
            "areaServed": "US"
        },
        "sameAs": [
            "https://facebook.com/softedigi",
            "https://twitter.com/softedigi",
            "https://linkedin.com/company/softedigi",
            "https://instagram.com/softedigi"
        ],
        "hasOfferCatalog": {
            "@type": "OfferCatalog",
            "name": "Software Development Services",
            "itemListElement": [
                {
                    "@type": "Offer",
                    "itemOffered": {
                        "@type": "Service",
                        "name": "Web Design",
                        "description": "Custom responsive web design and UI/UX solutions"
                    }
                },
                {
                    "@type": "Offer",
                    "itemOffered": {
                        "@type": "Service",
                        "name": "Software Development",
                        "description": "Full-stack software development and enterprise solutions"
                    }
                },
                {
                    "@type": "Offer",
                    "itemOffered": {
                        "@type": "Service",
                        "name": "Mobile App Development",
                        "description": "Native and cross-platform mobile applications"
                    }
                },
                {
                    "@type": "Offer",
                    "itemOffered": {
                        "@type": "Service",
                        "name": "Digital Marketing",
                        "description": "SEO, social media marketing, and digital strategy"
                    }
                }
            ]
        }
    }
    </script>

    <!-- Service-specific Schema (conditional) -->
    <% if (typeof serviceSchema !== 'undefined') { %>
    <script type="application/ld+json">
    <%- JSON.stringify(serviceSchema) %>
    </script>
    <% } %>
    
    <!-- SoftEdigi Favicon -->
    <link rel="icon" type="image/x-icon" href="/images/favicon.ico">
    <link rel="apple-touch-icon" sizes="180x180" href="/images/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/images/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/images/favicon-16x16.png">
    <link rel="manifest" href="/images/site.webmanifest">
    
    <!-- Preload critical resources -->
    <link rel="preload" href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700;800&display=swap" as="style">
    <link rel="preload" href="/css/style.css" as="style">
    
    <!-- DNS prefetch for external resources -->
    <link rel="dns-prefetch" href="//cdn.jsdelivr.net">
    <link rel="dns-prefetch" href="//cdnjs.cloudflare.com">
    <link rel="dns-prefetch" href="//fonts.googleapis.com">
    <link rel="dns-prefetch" href="//images.unsplash.com">
    
    <!-- Optimized Bootstrap CSS with integrity -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" 
          rel="stylesheet" 
          integrity="sha384-9ndCyUa6c4iuQ+jJKqp8RlHG8jNyuVPqH5PaQtkqlhC6t5+dGzjPAhIeeFh/AK/G" 
          crossorigin="anonymous">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css">
    <!-- Custom CSS -->
    <link rel="stylesheet" href="/css/style.css">
</head>



